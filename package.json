{
  "name": "cbn-bn",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "url": "https://github.com/ushkinaz/cbn-guide"
  },
  "author": {
    "name": "nornagon",
    "url": "https://github.com/nornagon"
  },
  "maintainers": [
    {
      "name": "MythosMod",
      "url": "https://github.com/mythosmod"
    },
    {
      "name": "ushkinaz",
      "url": "https://github.com/ushkinaz"
    }
  ],
  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "verify": "pnpm verify:format && pnpm verify:types",
    "verify:format": "prettier -c .",
    "verify:types": "svelte-check && tsc --noEmit",
    "fix:format": "prettier -w .",
    "test": "pnpm verify && vitest run src",
    "test:nightly": "pnpm fetch:fixtures:nightly && pnpm test",
    "test:watch": "pnpm fetch:fixtures && pnpm verify:types && vitest run src --watch",
    "fetch:fixtures": "tsx scripts/fetch-fixtures.ts",
    "fetch:fixtures:nightly": "tsx scripts/fetch-fixtures.ts nightly",
    "fetch:builds": "tsx scripts/fetch-builds.ts",
    "fetch:icons": "tsx scripts/fetch-icons.ts",
    "gen:css": "tsx scripts/gen-css.ts",
    "gen:sitemap": "tsx scripts/gen-sitemap.ts",
    "gen:unifont": "python3 scripts/gen-unifont.py",
    "gen:unifont:verify": "python3 scripts/gen-unifont.py --verify",
    "bench:node": "tsx scripts/bench-node.ts",
    "bench:browser": "tsx scripts/bench-browser.ts",
    "bench:browser:batch": "tsx scripts/bench-browser-batch.ts",
    "bench:report": "tsx scripts/bench-report.ts",
    "i18n:push": "txjs-cli push dist",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/traverse": "^7.28.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/svelte": "^4.2.3",
    "@transifex/cli": "^7.1.5",
    "@tsconfig/svelte": "^5.0.6",
    "@types/gettext.js": "^2.0.0",
    "@types/node": "^22.19.7",
    "@types/requestidlecallback": "^0.3.7",
    "ajv": "^8.17.1",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "puppeteer": "^24.35.0",
    "sharp": "^0.33.5",
    "svelte": "^4.2.20",
    "svelte-check": "^4.3.5",
    "ts-json-schema-generator": "^2.4.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "undici": "^7.18.2",
    "vite": "^5.4.21",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17",
    "workbox-build": "^7.4.0"
  },
  "dependencies": {
    "@sentry/browser": "^10.34.0",
    "@transifex/native": "^7.1.5",
    "fuzzysort": "^3.1.0",
    "gettext.js": "^2.0.3",
    "workbox-window": "^7.4.0"
  }
}
