    "transifex-push": "txjs-cli push dist",
    "lint": "prettier -c .",
    "lint:fix": "prettier -w .",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@fontsource/unifont": "^4.5.5",
