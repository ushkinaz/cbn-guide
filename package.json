  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "verify": "yarn verify:format && yarn verify:types",
    "verify:format": "prettier -c .",
    "verify:types": "svelte-check && tsc --noEmit",
    "fix:format": "prettier -w .",
    "test": "yarn verify && vitest run src",
    "test:latest": "yarn fetch:fixtures:latest && yarn test",
    "test:watch": "yarn fetch:fixtures && yarn verify:types && vitest run src --watch",
    "fetch:fixtures": "yarn fetch:builds && tsx scripts/fetch-fixtures.ts",
    "fetch:fixtures:latest": "yarn fetch:builds && tsx scripts/fetch-fixtures.ts latest",
    "fetch:builds": "tsx scripts/fetch-builds.ts",
    "fetch:icons": "tsx scripts/fetch-icons.ts",
    "gen:css": "tsx scripts/gen-css.ts",
    "gen:sitemap": "tsx scripts/gen-sitemap.ts",
    "gen:unifont": "sh scripts/gen-unifont.sh",
    "i18n:push": "txjs-cli push dist",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/traverse": "^7.26.10",
    "@sveltejs/vite-plugin-svelte": "^3",
    "@testing-library/svelte": "^4",
    "@transifex/cli": "^7.1.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/gettext.js": "^2.0.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.19.3",
    "@types/requestidlecallback": "^0.3.7",
    "ajv": "^8.17.1",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sharp": "^0.33.5",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.4",
    "ts-json-schema-generator": "^2.3.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "undici": "^7.10.0",
    "vite": "^5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "workbox-build": "^7.3.0"
  },
  "dependencies": {
    "@sentry/browser": "^10.32.1",
    "@sentry/tracing": "^7.120.4",
    "@transifex/native": "^7.1.5",
    "fuzzysort": "^3.1.0",
    "gettext.js": "^2.0.3",
    "lodash": "^4.17.21",
    "workbox-window": "^7.4.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "resolutions": {
    "esbuild": ">=0.25.0"
  }
}
