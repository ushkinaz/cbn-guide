    "@testing-library/svelte": "4",
    "@transifex/cli": "^4.2.4",
    "@transifex/native": "^4.2.4",
    "@tsconfig/svelte": "^2.0.1",
    "@types/gettext.js": "^1.0.3",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "18",
    "@types/node-fetch": "^2.6.12",
    "@types/requestidlecallback": "^0.3.7",
